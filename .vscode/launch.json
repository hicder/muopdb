      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=cli",
          "--package=muopdb"
        ],
        "filter": {
          "name": "cli",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
