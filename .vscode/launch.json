        ],
        "filter": {
          "name": "aggregator",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'cli'",
      "cargo": {
        "args": [
          "build",
          "--bin=cli",
          "--package=muopdb"
        ],
        "filter": {
          "name": "cli",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'cli' with info log",
      "cargo": {
        "args": [
          "build",
          "--bin=cli",
          "--package=muopdb"
