  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'muopdb'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--lib",
          "--package=muopdb"
        ],
        "filter": {
          "name": "muopdb",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'aggregator'",
      "cargo": {
        "args": [
          "build",
          "--bin=aggregator",
          "--package=muopdb"
        ],
        "filter": {
          "name": "aggregator",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'aggregator' with port 9001",
      "cargo": {
        "args": [
          "build",
          "--bin=aggregator",
          "--package=muopdb"
        ],
        "filter": {
          "name": "aggregator",
          "kind": "bin"
        }
      },
      "args": ["-p", "9001"],
      "env": {
        "RUST_LOG": "info"
      },
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'aggregator'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
